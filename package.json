{
    "name": "TestCAPJobTrigger",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@sap/cds": "^6",
        "express": "^4",
        "hdb": "^0.19.0",
        "@sap/cds-odata-v2-adapter-proxy": "^1"
    },
    "devDependencies": {
        "@sap/ux-specification": "^1.108.2",
        "rimraf": "^3.0.2",
        "sqlite3": "^5.0.4"
    },
    "scripts": {
        "start": "cds run",
        "watch-testcapjobtriggerui1": "cds watch --open testcapjobtriggerui1/webapp/index.html?sap-ui-xx-viewCache=false",
        "undeploy": "cf undeploy TestCAPJobTrigger --delete-services --delete-service-keys --delete-service-brokers",
        "build": "rimraf resources mta_archives && mbt build --mtar archive",
        "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
        "watch-anothertestcapui": "cds watch --open anothertestcapui/webapp/index.html?sap-ui-xx-viewCache=false"
    },
    "cds": {
        "requires": {
            "db": {
                "kind": "hana-cloud"
            }
        }
    },
    "sapux": [
        "app/testcapjobtriggerui1",
        "app/anothertestcapui"
    ]
}